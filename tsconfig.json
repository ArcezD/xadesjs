{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "lib": [
            "es5",
            "dom",
            "es2015.promise"
        ],
        "noImplicitAny": true,
        "sourceMap": false,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "strictNullChecks": true,
        "pretty": true,
        "noImplicitReturns": true,
        "rootDir": "src",
        "outFile": "build/xades.js"
    },
    "exclude": [
        "node_modules"
    ]
}